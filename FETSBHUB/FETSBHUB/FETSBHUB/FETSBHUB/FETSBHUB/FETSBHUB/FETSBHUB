local Tab = Window:NewTab("Info")

local Section = Tab:NewSection("Credit")

Section:NewLabel("youtube: The Editer Man + Scripter")

Section:NewLabel("Roblox: MyBatteryIsGetit")

-- Create a ScreenGui
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = game.CoreGui

-- Create a Frame to act as the button
local Button = Instance.new("TextButton")
Button.Parent = ScreenGui
Button.Size = UDim2.new(0, 150, 0, 50) -- Width: 150px, Height: 50px
Button.Position = UDim2.new(0, 0, 0.5, -25) -- Middle-left, 25px offset upwards
Button.Text = "FE TSB HUB"
Button.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2) -- Dark gray
Button.TextColor3 = Color3.new(1, 1, 1) -- White text
Button.Font = Enum.Font.SourceSansBold
Button.TextSize = 18

-- Add functionality to the button
Button.MouseButton1Click:Connect(function()
Library:ToggleUI()
end)

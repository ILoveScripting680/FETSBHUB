local Tab = Window:NewTab("Dummy Stuff")

local Section = Tab:NewSection("Dummy")

Section:NewToggle("Toggle view dummy", "Toggle to view the dummy.", function(state)
    if state then
        game.Workspace.Camera.CameraSubject = game.Workspace.Live:FindFirstChild("Weakest Dummy").Humanoid
    else
        game.Workspace.Camera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
    end
end)

